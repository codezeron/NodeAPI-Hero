{
  "compilerOptions": {
		"target": "ES2022",
		"module": "CommonJS",
		"lib": [
			"ES2020"
		],
		"allowJs": true,
		"outDir": "dist",
		"rootDir": "src",
		"strict": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"alwaysStrict": true,
		"noImplicitAny": false,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"strictPropertyInitialization": false
	}
}
